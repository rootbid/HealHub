from huggingface_hub import hf_hub_download
import pandas as pd

file_path = hf_hub_download(
    repo_id="QuyenAnhDE/Diseases_Symptoms",
    filename="Diseases_Symptoms.csv",
    repo_type="dataset"
)

df = pd.read_csv(file_path)

print("Sample rows:")
print(df.head())
